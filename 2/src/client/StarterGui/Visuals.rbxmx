		<Item class="LocalScript" referent="RBXFA1021C35D8D4E0BA4CC2146B969D71C">
			<Properties>
				<ProtectedString name="Source"><![CDATA[local Players = game:GetService("Players")
local player = Players.LocalPlayer

local gui = script.Parent
local label = gui:WaitForChild("ScoreLabel")

local function hook()
	local stats = player:WaitForChild("leaderstats")
	local score = stats:WaitForChild("Score")

	label.Text = "Score: " .. score.Value
	score.Changed:Connect(function()
		label.Text = "Score " .. score.Value
	end)
end

hook()
]]></ProtectedString>
